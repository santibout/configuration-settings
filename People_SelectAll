ALTER PROCEDURE [dbo].[People_SelectAll]
/*
	EXECUTE [dbo].[People_SelectAll]
*/
AS
	BEGIN

		SELECT 
			[Id],
			[FirstName],
			[MiddleInitial],
			[LastName],
			[DateOfBirth],
			[CreatedDate],
			[ModifiedDate],
			[ModifiedBy]
		FROM
			[dbo].[People]
END
